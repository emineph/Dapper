@model IEnumerable<DapperProje.Models.SevkiyatModels>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Yeni Ekle", "EY")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SevkiyatYeri)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SevkiyatTarihi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SirketAdi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefon)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SevkiyatYeri)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SevkiyatTarihi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SirketAdi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefon)
        </td>
        <td>
            @Html.ActionLink("Edit", "EY", new {  id=item.SevkiyatNo }) |
          
            @Html.ActionLink("Delete", "Delete", new {  id=item.SevkiyatNo  })
        </td>
    </tr>
}

</table>
